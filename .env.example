# Firebase Configuration - DEVELOPMENT EXAMPLE
# Copy this file to .env and fill with your Firebase project keys
# NEVER commit actual .env files to git!

# Firebase Project Configuration
FIREBASE_PROJECT_ID=mediexchange
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here

# Web Platform
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here

# Android Platform  
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here

# iOS 1
FIREBASE_IOS_API_KEY=your_ios_api_key_here  
FIREBASE_IOS_APP_ID=your_ios_app_id_here

# Backend Configuration
FIREBASE_FUNCTIONS_REGION=europe-west1
FIREBASE_FUNCTIONS_URL=https://europe-west1-mediexchange.cloudfunctions.net